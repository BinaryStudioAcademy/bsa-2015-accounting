#personal-view
  margin-top 50px
  	
	// Table styles
	#tbody-container
		overflow auto
		height 250px
		width 95%
	#table-container
		width 100%
		i, a
			cursor pointer
		#delete-button
			margin-left 10px
	table
		width 95%
		margin-left 0px
		&.fixed
			table-layout fixed
	.table-edit,
	.table-expense,
	.table-cost,
	.table-carrency,
	.table-category,
	.table-sub,
	.table-addBy,
	.table-budget,
	.table-left
		width 100px
	.table-description
		width 70px
	select
		height 36px
	#income-header
		background lightgreen
		font-weight 700
	#outcome-header
		background darksalmon
		font-weight 700
	#income-table
		margin-bottom 5px
	#budget-left
		color #008000
		font-weight 700
	#budget-spent
		color #f00
		font-weight 700
	#currency-group
		.btn
			font-size 10px !important
			padding 0px 8px 0px 8px !important
		.btn-primary
			background-color #fff !important
			color #337ab7 !important
			border-color #ddd !important
		.active
			color #fff !important
			background-color #286090 !important
			border-color #204d74 !important
		
