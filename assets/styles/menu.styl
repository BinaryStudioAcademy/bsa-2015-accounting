#gen-view
	margin-left 203px
#menu-view
	body
		min-width 320px
		max-width 1920px
	h2
		margin-top 10px

	.hide-me
		display block

	main > .tab-content
		margin-top: 75px;

	// header
	// 	line-height 50px
	// 	background #1E88E5
	// 	position fixed
	// 	width 100%
	// 	height 52px
	// 	z-index 9999

	header#header
		position fixed
		width 100%
		z-index 200

	nav
		background white
		z-index 100
		margin-top 70px
		width 200px
		float left
		position fixed
		top 0
		//bottom 0
		ul
			height 100%

	main
		max-width 1920px
		//margin-left 200px
		padding 5px

	#page-name
		text-align center
		font-size 30px
		color white
		font-weight 700
		padding-right 125px
		margin-left 45px
	
	#user-info
		font-size 15px
		font-weight 600
		color white
		position absolute
		top 0
		right 10px
		cursor pointer
		a
			color white
			text-decoration none
	#menu-btn
		color white
		font-size 29px
		padding 0 10px
		position absolute
		top 10px
		left 0
		cursor pointer
		display none

	.nav-tabs
		border none
		& > li
			float none

	.nav-tabs
		& > li
			& > a ,.nav-tabs 
				border none
				border-radius 0
	.active-tab
		background -webkit-linear-gradient(left, #e2e2e2 0%,#fefefe 100%)

	.nav>li>a:focus, .nav>li>a:hover
		background -webkit-linear-gradient(left, #e2e2e2 0%,#fefefe 100%);
	.showMenu
		display initial !important
		
	#menu-btn-wrapper
		background #ffa800
		height 30px
		width 30px
		border-radius 5px
		text-align center
		margin-top 20px
		position absolute
		left 20px
		cursor pointer
		display none
		i
			color white
			font-size 24px
			line-height 30px
	

	@media screen and (max-width: 1235px)
		#gen-view
			margin-left 3px
			margin-top 115px
		nav
			margin-top 70px
			width 100%
			ul
				text-align center
			.active-tab
				background -webkit-linear-gradient(top, #e2e2e2 0%,#fefefe 100%)
			li
				display inline-block

			.nav>li>a:focus, .nav>li>a:hover
				background -webkit-linear-gradient(top, #e2e2e2 0%,#fefefe 100%)

	@media screen and (max-width: 666px)
		.hide-me
			display block
		#gen-view
			margin-top 0
		#menu-btn-wrapper
			display block
		nav
			display none
			li
				display block
				border-bottom 1px solid #e2e2e2
		#BSheaderLogo
			visibility hidden

